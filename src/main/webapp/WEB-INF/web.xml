<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>AddTeacher</servlet-name>
  	<display-name>AddTeacher</display-name>
  	<description></description>
  	<servlet-class>com.admin.servlet.AddTeacher</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Assign_course</servlet-name>
  	<display-name>Assign_course</display-name>
  	<description></description>
  	<servlet-class>com.admin.servlet.Assign_course</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>AddTeacher</servlet-name>
  	<url-pattern>/AddTeacher</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Assign_course</servlet-name>
  	<url-pattern>/Assign_course</url-pattern>
  </servlet-mapping>
</web-app>
